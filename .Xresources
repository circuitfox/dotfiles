#include<.xresources/theme>

! special
URxvt*foreground:   fg
URxvt*background:   bg
URxvt*cursorColor:  cColor

! black
URxvt*color0:       black
URxvt*color8:       lblack

! red
URxvt*color1:       red
URxvt*color9:       lred

! green
URxvt*color2:       green
URxvt*color10:      lgreen

! yellow
URxvt*color3:       yellow
URxvt*color11:      lyellow

! blue
URxvt*color4:       blue
URxvt*color12:      lblue

! magenta
URxvt*color5:       magenta
URxvt*color13:      lmagenta

! cyan
URxvt*color6:       cyan
URxvt*color14:      lcyan

! white
URxvt*color7:       grey
URxvt*color15:      white

! no scrollbar
URxvt*scrollBar:    false

! font
URxvt*font: xft:Source Code Pro:pixelsize=14:style=medium

! cursor
Xcursor.theme: oxygen
